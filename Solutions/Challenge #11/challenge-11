function getCompleted(part, total) {
  let resultado = 0
  let trabajado = part.split(":")[0] * 3600 + part.split(":")[1] * 60 + part.split(":")[2]
  let total = total.split(":")[0] * 3600 + total.split(":")[1] * 60 + total.split(":")[2]
  if (trabajado === total)
    return '1/1'
  resultado = trabajado / total
  return ''
}